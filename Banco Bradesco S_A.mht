/* STEPS */
.steps {height:48px;clear:both;}
.steps_nh {height:auto !important;clear:both;}
.steps li {float:left;position:relative;margin-right:11px;text-align:center;color:#999999;}
.mioloFs13 .steps li,
.UIFontSize-level-2 .UISequenciaPasso-steps li {font-size:1.1em;}
.mioloFs14 .steps li,
.UIFontSize-level-3 .UISequenciaPasso-steps li {font-size:1.2em;}
.steps li div {float:left;background:url(../../../imagens/geral/bgr_steps.jpg) 100% -129px no-repeat;padding-right:8px;}
.steps li strong {float:left;display:block;height:24px;line-height:24px;border-bottom:solid 2px #CCCCCC;padding:0 4px 0 3px;letter-spacing:-0.04em;}
.steps li.last {margin-right:0;}
.steps li.last div {margin-right:0;background-position:100% -179px;padding-right:2px;}

.steps li .active {background-position:100% -29px;color:#000000;}
.steps li .active strong {border-color:#900000;padding-left:27px;}
.steps li .active span {position:absolute;display:block;color:#FFFFFF;left:0;top:0;width:21px;height:24px;background:url(../../../imagens/geral/bgr_steps.jpg) no-repeat;font-weight:normal;}
.steps li .active em {display:block;padding-left:26px;} /* para usar quando o step 1 for, por exemplo, "Confirmao de Dados" */
.steps li.last .active {background-position:100% -79px;}

